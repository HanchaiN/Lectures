{
  "github": {
    "branch": "docs",
    "automaticallyMergePR": true,
    "api": {
      "tiersForApi": "Github Free/Pro/Team (default)",
      "hostname": ""
    },
    "otherRepo": [],
    "workflow": {
      "name": "",
      "commitMessage": "PUBLISHER"
    },
    "tokenPath": ".obsidian/plugins/obsidian-mkdocs-publisher/.env",
    "repo": "Lectures",
    "user": "HanchaiN",
    "rateLimit": 4946,
    "verifiedRepo": true
  },
  "upload": {
    "behavior": "obsidian",
    "subFolder": "",
    "defaultName": "docs",
    "rootFolder": "docs",
    "yamlFolderKey": "category",
    "frontmatterTitle": {
      "enable": true,
      "key": "name"
    },
    "replaceTitle": [],
    "autoclean": {
      "enable": true,
      "excluded": [
        "docs/assets/js",
        "docs/assets/meta",
        "docs/assets/css",
        "tags.md",
        "graph.md"
      ]
    },
    "folderNote": {
      "enable": true,
      "rename": "index.md"
    },
    "metadataExtractorPath": "",
    "replacePath": []
  },
  "conversion": {
    "hardbreak": false,
    "dataview": false,
    "censorText": [],
    "tags": {
      "inline": true,
      "exclude": [],
      "fields": []
    },
    "links": {
      "internal": true,
      "unshared": false,
      "wiki": false,
      "slugify": true
    }
  },
  "embed": {
    "attachments": true,
    "keySendFile": [
      "image"
    ],
    "notes": true,
    "folder": "docs/assets/img"
  },
  "plugin": {
    "shareKey": "share",
    "fileMenu": true,
    "editorMenu": true,
    "excludedFolder": [
      "Deprecated"
    ],
    "copyLink": {
      "enable": false,
      "links": "",
      "removePart": [],
      "addCmd": false
    },
    "noticeError": false,
    "displayModalRepoEditing": false
  }
}